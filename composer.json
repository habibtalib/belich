{
    "name": "daguilarm/belich",
    "description": "Laravel-vuejs admin dashboard",
    "keywords": ["framework", "laravel","dashboard","vuejs", "admin"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "daguilarm",
            "email": "damian.aguilarm@gmail.com"
        }
    ],
    "require": {
        "proengsoft/laravel-jsvalidation": ">2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4@dev"
    },
    "autoload": {
        "psr-4": {
            "Daguilarm\\Belich\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "php artisan vendor:publish --provider=\"Proengsoft\\JsValidation\\JsValidationServiceProvider\" --tag=public --force"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daguilarm\\Belich\\BelichServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
